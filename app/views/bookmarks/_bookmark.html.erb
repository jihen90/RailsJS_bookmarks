    <% @bookmarks.each do |bookmark| %>
        <tr>
        <td><%= bookmark.name %></td>
        <td><%= bookmark.url %></td>
        <td><%= bookmark.category.name %></td>
        <td><%= bookmark.category.visibility %></td>
        <td><%= bookmark.type.name %></td>
        <td><%= link_to 'Show', bookmark %></td>
        <td><%= link_to 'Edit', edit_bookmark_path(bookmark) %></td>
        <td><%= link_to 'Destroy', bookmark, method: :delete, data: {confirm: 'Â¿Quieres eliminar el marcador?'} %></td>
        </tr>
    <% end %>